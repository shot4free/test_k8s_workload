---

registry:
  image:
    tag: v2.9.1-gitlab

gitlab:
  mailroom:
    enabled: false

global:
  appConfig:
    incomingEmail:
      enabled: true
    omniauth:
      providers:
        - secret: gitlab-google-oauth2-v1
          key: provider
  email:
    reply_to: noreply@gitlab.com

  hosts:
    gitlab:
      name: pre.gitlab.com

  smtp:
    enabled: false
    password:
      key: secret
      secret: gitlab-smtp-credential-v1
